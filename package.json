{
    "name": "developer-theme-dark",
    "displayName": "Developer's theme",
    "description": "A theme suitable to teach code online in dark mode",
    "version": "1.6.2",
    "publisher": "Rajeshwaran",
    "author": {
        "name": "Rajeshwaran"
    },
    "icon": "assest/logo.ico",
    "repository": {
        "type": "git",
        "url": "https://github.com/Rajeshwaran2001/developer-theme-dark.git"
    },
    "galleryBanner": {
        "color": "#650681",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "Dark Theme",
        "Code Tutorial",
        "Developer's Dark Theme",
        "theme",
        "Coders Theme",
        "Programmers Theme",
        "Dark Theme lovers",
        "color-theme",
        "dark",
        "icon",
        "product"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Developers Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/Developers Theme dark-color-theme.json"
            }
        ],
        "productIconThemes": [
            {
                "id": "developer-icons",
                "label": "Developer's Icons",
                "path": "./product_icons/developer's-icons.json"
            }
        ]
    },
    "scripts": {
        "pre": "vsce publish --pre-release --no-yarn",
        "minor": "vsce publish minor --no-yarn",
        "major": "vsce publish major --no-yarn",
        "patch": "vsce publish patch --no-yarn",
        "pkg": "vsce package --no-yarn",
        "release": "release-it",
        "clean": "clean.bat",
        "svgo": "svgo -f src/icons/ --config config.yml",
        "export-to-csv": "node src/export/index.js -f dist/developer-icons.ttf > dist/developer-icons.csv",
        "fonts": "icon-font-generator src/icons/*.svg -o dist -n developer-icons -p developer-icons --csstp src/template/styles.hbs --htmltp src/template/preview.hbs --height=1000 --center --codepoints src/template/mapping.json --json false --types 'ttf'",
        "copy": "copy.bat",
        "dev": "npm run clean && npm run svgo && npm run fonts && npm run export-to-csv && npm run copy"
    },
    "devDependencies": {
        "handlebars": ">=4.7.7",
        "icon-font-generator": "^2.1.10",
        "lodash": ">=4.17.21",
        "minimist": "^1.2.3",
        "opentype.js": "^0.6.6",
        "svgo": "1.3.2",
        "underscore": ">=1.12.1",
        "xmldom": ">=0.5.0"
    }
}
